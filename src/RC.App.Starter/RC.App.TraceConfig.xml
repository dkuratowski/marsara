<traceConfig>
  <registerTraceFilter namespace="RC.App" name="Info" />

  <activateTraceFilters pattern="RC.App.Info" />
  <activateTraceFilters pattern="RC.UI.Error" />
  <activateTraceFilters pattern="RC.UI.Warning" />
  <activateTraceFilters pattern="RC.UI.Info" />
  <activateTraceFilters pattern="RC.UI.Details" />
  <activateTraceFilters pattern="RC.UI.XnaPlugin.Error" />
  <activateTraceFilters pattern="RC.UI.XnaPlugin.Warning" />
  <activateTraceFilters pattern="RC.UI.XnaPlugin.Info" />
  <activateTraceFilters pattern="RC.UI.XnaPlugin.Details" />
  <activateTraceFilters pattern="RC.Engine.Maps.Error" />
  <activateTraceFilters pattern="RC.Engine.Maps.Warning" />
  <activateTraceFilters pattern="RC.Engine.Maps.Info" />
  <activateTraceFilters pattern="RC.Engine.Maps.Details" />
  <activateTraceFilters pattern="RC.Engine.Scenarios.Error" />
  <activateTraceFilters pattern="RC.Engine.Scenarios.Warning" />
  <activateTraceFilters pattern="RC.Engine.Scenarios.Info" />
  <activateTraceFilters pattern="RC.Engine.Scenarios.Details" />
  <activateTraceFilters pattern="RC.Common.ComponentMgr.Info" />
  <activateTraceFilters pattern="RC.App.PresLogic.Info" />
  <activateTraceFilters pattern="RC.App.BizLogic.Info" />

  <registerTrace>
    <assembly>RC.Common, Version=0.0.0.0, Culture=neutral, PublicKeyToken=null</assembly>
    <class>RC.Common.Diagnostics.ConsoleTrace</class>
    <ctorParam type="STRING">RC.App.ConsoleTrace</ctorParam>
  </registerTrace>

