    <resource namespace="RC.App.Sprites" name="PlanetBackground" loader="SpriteLoader">
      <path name="imageFile">../sprites/menusprites/planet_background.png</path>
      <parameter name="pixelSize">$(RC.UI.UIWorkspace.PixelScaling)</parameter>
    </resource>

    <resource namespace="RC.App.Sprites" name="ScrollbarHorz" loader="SpriteLoader">
      <path name="imageFile">../sprites/controls/scrollbar_horz.png</path>
      <parameter name="pixelSize">$(RC.UI.UIWorkspace.PixelScaling)</parameter>
    </resource>
    <resource namespace="RC.App.Sprites" name="ScrollbarVert" loader="SpriteLoader">
      <path name="imageFile">../sprites/controls/scrollbar_vert.png</path>
      <parameter name="pixelSize">$(RC.UI.UIWorkspace.PixelScaling)</parameter>
