<resourceFile>
  <loader name="SpriteLoader" assembly="RC.UI, Version=0.0.0.0, Culture=neutral, PublicKeyToken=null" class="RC.UI.UISpriteLoader" />
  <loader name="FontLoader" assembly="RC.UI, Version=0.0.0.0, Culture=neutral, PublicKeyToken=null" class="RC.UI.UIFontLoader" />
  <loader name="AnimationLoader" assembly="RC.UI, Version=0.0.0.0, Culture=neutral, PublicKeyToken=null" class="RC.UI.UIAnimationLoader" />

  <resourceGroup namespace="RC.App" name="SplashScreen">
    <resource namespace="RC.App.Sprites" name="Font6Sprite" loader="SpriteLoader">
      <path name="imageFile">../fonts/font6.png</path>
    </resource>
    <resource namespace="RC.App.Sprites" name="Font7Sprite" loader="SpriteLoader">
      <path name="imageFile">../fonts/font7.png</path>
    </resource>
    
    <resource namespace="RC.App.Sprites" name="MainMenuTitle0" loader="SpriteLoader">
      <path name="imageFile">../sprites/menusprites/mainmenu_title_phase0.png</path>
      <parameter name="transparentColor">255;0;255</parameter>
      <parameter name="pixelSize">$(RC.UI.UIWorkspace.PixelScaling)</parameter>
    </resource>
    <resource namespace="RC.App.Sprites" name="MainMenuTitle1" loader="SpriteLoader">
      <path name="imageFile">../sprites/menusprites/mainmenu_title_phase1.png</path>
      <parameter name="transparentColor">255;0;255</parameter>
      <parameter name="pixelSize">$(RC.UI.UIWorkspace.PixelScaling)</parameter>
    </resource>
    <resource namespace="RC.App.Sprites" name="MainMenuTitle2" loader="SpriteLoader">
      <path name="imageFile">../sprites/menusprites/mainmenu_title_phase2.png</path>
      <parameter name="transparentColor">255;0;255</parameter>
      <parameter name="pixelSize">$(RC.UI.UIWorkspace.PixelScaling)</parameter>
    </resource>
    <resource namespace="RC.App.Sprites" name="MainMenuTitleAll" loader="SpriteLoader">
      <path name="imageFile">../sprites/menusprites/mainmenu_title_all.png</path>
      <parameter name="transparentColor">255;0;255</parameter>
      <parameter name="pixelSize">$(RC.UI.UIWorkspace.PixelScaling)</parameter>
    </resource>
    <resource namespace="RC.App.Sprites" name="MainMenuBackground" loader="SpriteLoader">
      <path name="imageFile">../sprites/menusprites/mainmenu_background.png</path>
      <parameter name="pixelSize">$(RC.UI.UIWorkspace.PixelScaling)</parameter>
    </resource>

    <resource namespace="RC.App.Fonts" name="Font6" loader="FontLoader">
      <path name="mappingFile">../fonts/font6.xml</path>
      <parameter name="fontSprite">RC.App.Sprites.Font6Sprite</parameter>
    </resource>
    <resource namespace="RC.App.Fonts" name="Font7" loader="FontLoader">
      <path name="mappingFile">../fonts/font7.xml</path>
      <parameter name="fontSprite">RC.App.Sprites.Font7Sprite</parameter>
    </resource>
    
    <resource namespace="RC.App.Animations" name="MainMenuTitleAnim" loader="AnimationLoader">
      <path name="animationFile">../animations/mainmenu_title_anim.xml</path>
    </resource>
  </resourceGroup>
  
