<resourceFile>
  <loader name="SpriteLoader" assembly="RC.UI, Version=0.0.0.0, Culture=neutral, PublicKeyToken=null" class="RC.UI.UISpriteLoader" />
  <loader name="FontLoader" assembly="RC.UI, Version=0.0.0.0, Culture=neutral, PublicKeyToken=null" class="RC.UI.UIFontLoader" />
  <loader name="AnimationLoader" assembly="RC.UI, Version=0.0.0.0, Culture=neutral, PublicKeyToken=null" class="RC.UI.UIAnimationLoader" />

  <resourceGroup namespace="RC.MapEditor" name="Resources">
    <resource namespace="RC.MapEditor.Sprites" name="MenuPointerSprite" loader="SpriteLoader">
      <path name="imageFile">../sprites/pointers/menu_pointer.png</path>
      <parameter name="transparentColor">255;0;255</parameter>
      <parameter name="pixelSize">$(RC.UI.UIWorkspace.PixelScaling)</parameter>
    </resource>
    <resource namespace="RC.App.Sprites" name="ScrollbarHorz" loader="SpriteLoader">
      <path name="imageFile">../sprites/controls/scrollbar_horz.png</path>
      <parameter name="pixelSize">$(RC.UI.UIWorkspace.PixelScaling)</parameter>
    </resource>
    <resource namespace="RC.App.Sprites" name="ScrollbarVert" loader="SpriteLoader">
      <path name="imageFile">../sprites/controls/scrollbar_vert.png</path>
      <parameter name="pixelSize">$(RC.UI.UIWorkspace.PixelScaling)</parameter>
    </resource>
    <resource namespace="RC.App.Sprites" name="DropdownSelector" loader="SpriteLoader">
      <path name="imageFile">../sprites/controls/dropdown.png</path>
      <parameter name="pixelSize">$(RC.UI.UIWorkspace.PixelScaling)</parameter>
    </resource>
    <resource namespace="RC.App.Sprites" name="ListBox" loader="SpriteLoader">
      <path name="imageFile">../sprites/controls/listbox.png</path>
      <parameter name="pixelSize">$(RC.UI.UIWorkspace.PixelScaling)</parameter>
    </resource>
    <resource namespace="RC.MapEditor.Sprites" name="CtrlPanel" loader="SpriteLoader">
      <path name="imageFile">../sprites/menusprites/mapeditor_ctrlpanel.png</path>
	    <parameter name="transparentColor">255;0;255</parameter>
      <parameter name="pixelSize">$(RC.UI.UIWorkspace.PixelScaling)</parameter>
    </resource>
    <resource namespace="RC.MapEditor.Sprites" name="IsotileHighlighted" loader="SpriteLoader">
      <path name="imageFile">../sprites/controls/isotile_highlighted.png</path>
      <parameter name="transparentColor">255;0;255</parameter>
      <parameter name="pixelSize">$(RC.UI.UIWorkspace.PixelScaling)</parameter>
    </resource>
    <resource namespace="RC.MapEditor.Sprites" name="IsotileNormal" loader="SpriteLoader">
      <path name="imageFile">../sprites/controls/isotile_normal.png</path>
      <parameter name="transparentColor">255;0;255</parameter>
      <parameter name="pixelSize">$(RC.UI.UIWorkspace.PixelScaling)</parameter>
    </resource>
    <resource namespace="RC.App.Sprites" name="ObjectPlacementMaskGreen" loader="SpriteLoader">
      <path name="imageFile">../sprites/controls/object_placement_mask_green.png</path>
      <parameter name="transparentColor">255;0;255</parameter>
      <parameter name="pixelSize">$(RC.UI.UIWorkspace.PixelScaling)</parameter>
    </resource>
    <resource namespace="RC.App.Sprites" name="ObjectPlacementMaskRed" loader="SpriteLoader">
      <path name="imageFile">../sprites/controls/object_placement_mask_red.png</path>
      <parameter name="transparentColor">255;0;255</parameter>
      <parameter name="pixelSize">$(RC.UI.UIWorkspace.PixelScaling)</parameter>
    </resource>

    <resource namespace="RC.App.Fonts" name="Font5" loader="FontLoader">
      <path name="mappingFile">../fonts/font5.xml</path>
      <path name="imageFile">../fonts/font5.png</path>
    </resource>
    <resource namespace="RC.MapEditor.Fonts" name="Font6" loader="FontLoader">
      <path name="mappingFile">../fonts/font6.xml</path>
      <path name="imageFile">../fonts/font6.png</path>
    </resource>
    <resource namespace="RC.App.Fonts" name="Font9B" loader="FontLoader">
      <path name="mappingFile">../fonts/font9b.xml</path>
      <path name="imageFile">../fonts/font9b.png</path>
    </resource>
  </resourceGroup>
</resourceFile>